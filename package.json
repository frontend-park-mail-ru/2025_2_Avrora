{
  "name": "homa",
  "version": "1.0.0",
  "description": "Homa Frontend",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "dev": "vite",
    "build": "npm run compile-hbs && vite build",
    "preview": "vite preview",
    "lint": "eslint . --color",
    "lint:report": "eslint . --format stylish",
    "lint:fix": "eslint . --fix",
    "docs": "jsdoc -c jsdoc.json",
    "build-css": "sass public/styles/index.scss public/styles/index.css",
    "watch-css": "sass --watch public/styles/index.scss public/styles/index.css",
    "build-css-prod": "sass public/styles/index.scss public/styles/index.css --style=compressed",
    "compile-hbs": "node compile-templates.js"
  },
  "keywords": [
    "spa",
    "realestate",
    "api",
    "express"
  ],
  "author": "Homa",
  "license": "MIT",
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "esbuild": "^0.25.11",
    "eslint": "^9.36.0",
    "jsdoc": "^4.0.4",
    "sass": "^1.93.2",
    "vite": "^7.1.12"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}